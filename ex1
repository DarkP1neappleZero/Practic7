rating = int(input("–í–≤–µ–¥–∏—Ç–µ —Ä–µ–π—Ç–∏–Ω–≥ –ø—Ä–æ–¥—É–∫—Ç–∞ –æ—Ç 1 –¥–æ 10: "))

match rating:
        case 10 | 9 | 8:
            print("–û—Ç–ª–∏—á–Ω—ã–π –ø—Ä–æ–¥—É–∫—Ç! üåü –ù–∞—Å—Ç–æ—è—Ç–µ–ª—å–Ω–æ —Ä–µ–∫–æ–º–µ–Ω–¥—É—é!")
        case 7 | 6:
            print("–•–æ—Ä–æ—à–∏–π –ø—Ä–æ–¥—É–∫—Ç! üò≤ –†–µ–∫–æ–º–µ–Ω–¥—É–µ–º.")
        case 5 | 4:
            print("–°—Ä–µ–¥–Ω–∏–π –ø—Ä–æ–¥—É–∫—Ç! üôè –ú–æ–∂–Ω–æ –ø–æ–ø—Ä–æ–±–æ–≤–∞—Ç—å.")
        case 3 | 2 | 1:
            print("–ü–ª–æ—Ö–æ–π –ø—Ä–æ–¥—É–∫—Ç! üëé –ù–µ —Ä–µ–∫–æ–º–µ–Ω–¥—É—é.")
        case _:
            print("–û—à–∏–±–∫–∞: —Ä–µ–π—Ç–∏–Ω–≥ –¥–æ–ª–∂–µ–Ω –±—ã—Ç—å –æ—Ç 1 –¥–æ 10!")
